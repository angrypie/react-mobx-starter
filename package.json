{
	"name": "react-mobx-starter",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"mobx": "^5.15.1",
		"mobx-react-lite": "^1.5.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-jsx": "^3.2.4",
		"wouter": "^2.4.0"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build && rimraf ./build/static/**/*.map",
		"test": "craco test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"src/**/*.{js,jsx,css}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.7.7",
		"@craco/craco": "^5.6.2",
		"babel-plugin-root-import": "^6.4.1",
		"husky": "^3.1.0",
		"lint-staged": "^9.5.0",
		"prettier": "^1.19.1",
		"react-scripts": "^3.3.0",
		"rimraf": "^3.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,css}": [
			"prettier --write",
			"git add"
		]
	}
}
