{
	"name": "react-mobx-starter",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"mobx": "^5.15.4",
		"mobx-react-lite": "^2.0.7",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"styled-jsx": "^3.3.0",
		"wouter": "^2.5.0"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build && rimraf ./build/static/**/*.map",
		"test": "craco test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"src/**/*.{js,jsx,css}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.10.4",
		"@craco/craco": "^5.6.4",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"react-scripts": "^3.4.1",
		"rimraf": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,css}": [
			"prettier --write",
			"git add"
		]
	}
}
