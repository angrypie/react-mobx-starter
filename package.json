{
	"name": "react-mobx-starter",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"mobx": "^6.0.0",
		"mobx-react-lite": "^3.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"styled-jsx": "^3.3.0",
		"wouter": "^2.5.1"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build && rimraf ./build/static/**/*.map",
		"test": "craco test",
		"eject": "react-scripts eject",
		"format": "prettier --write \"src/**/*.{js,jsx,css}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.11.0",
		"@craco/craco": "^5.7.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"prettier": "^2.1.2",
		"react-scripts": "^3.4.3",
		"rimraf": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,css}": [
			"prettier --write"
		]
	}
}
